{
  "compilerOptions": {
    "module": "es2020",
    "target": "es2020",
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "importsNotUsedAsValues": "error",
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "strict": true,
    "strictNullChecks": true,
    "outDir": "dist"
  },
  "exclude": ["node_modules"]
}
